{
  "name": "basic_druc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:production": "tsc && node -r dotenv/config ./build/index.js",
    "start": "ts-node -r dotenv/config ./src/index.ts",
    "dev": "nodemon",
    "test": "mocha -p ./tsconfig.json src/**/*.spec.ts",
    "db:init": "node_modules/.bin/sequelize init",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "build:docker": "docker-compose -f docker-compose.dev.yml build --no-cache --force-rm",
    "build:test:docker": "docker-compose -f docker-compose.test.yml build --no-cache --force-rm",
    "start:docker:dev:up": "docker-compose -f docker-compose.dev.yml -p node-sql  up -d --no-deps --build --force-recreate --remove-orphans",
    "start:docker:dev:down": "docker-compose -f docker-compose.dev.yml -p node-sql down",
    "test:docker:up": "docker-compose -f docker-compose.test.yml -p node-sql  up -d --no-deps --build --force-recreate --remove-orphans",
    "test:docker:down": "docker-compose -f docker-compose.test.yml -p node-sql down --remove-orphans"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/expect": "^1.20.4",
    "@types/express": "^4.16.1",
    "@types/helmet": "0.0.42",
    "@types/mocha": "^5.2.6",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.9.5",
    "@types/sequelize": "^4.27.38",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^2.1.0",
    "helmet": "^3.15.1",
    "jwks-rsa": "^1.4.0",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "supertest": "^3.4.2",
    "uuid": "^3.3.2"
  }
}
